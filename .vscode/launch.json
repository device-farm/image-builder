    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "mic build",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/builder/src/start.js",
            "args": [
                "build",
                "defa/orangepi-zero:test",
                "orangepi-zero",
                "dropbear",
                "avahi"
            ],
            "outputCapture": "std"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "mic all",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/builder/src/start.js",
            "args": [
                "all"
            ],
            "outputCapture": "std"
        }
    ]
}